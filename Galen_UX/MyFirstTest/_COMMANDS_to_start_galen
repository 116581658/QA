galen check home-page.gspec  --url http://samples.galenframework.com/tutorial1/tutorial1.html  --size 640x480   --htmlreport .

galen dump home-page.gspec  --url http://samples.galenframework.com/tutorial1/tutorial1.html  --size 640x480   --export homepage-dump

galen dump home-page_dump.gspec  --url http://testapp.galenframework.com  --size 1024x768  --export homepage-dump